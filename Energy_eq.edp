include Naranjani_newton
fespace H(Th, P1);
macro grad(v) [dx(v), dy(v), dz(v)] //
H to=25, h=0; //da sistemare
H T, tau;
real Cp=1.92; //da sistemare
real lambda=2; //da sistemare

problem energy (T, tau)
    = int3d(Th)(rhoa*Cp*u'*grad(T)*tau + lamba*grad(T)'*grad(tau))
    -int2d(Th)(lambda*h*tau)
    +on(1, T=to);

energy;